    "react-masonry-component": "^6.3.0",
    "react-masonry-css": "^1.0.16",
    "react-meta-tags": "^1.0.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-quilljs": "^1.2.17",
    "react-rangeslider": "^2.2.0",
    "react-rating": "^2.0.5",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.2",
    "react-shepherd": "^3.3.6",
    "react-switch": "^6.0.0",
    "react-table": "^7.7.0",
    "react-toastify": "^8.1.1",
    "react-tsparticles": "^1.40.0",
    "reactstrap": "^9.0.1",
    "remixicon-react": "^1.0.0",
    "save": "^2.4.0",
    "simplebar-react": "^2.3.6",
    "swiper": "^8.0.6",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "build": "env-cmd -f .env react-scripts build --max_old_space_size=8000 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\""
  },
  "babelOptions": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "$": true
    },
    "settings": {
      "react": {
        "version": "^17.0.1"
      }
    },
    "rules": {
      "semi": 0,
      "react/no-unescaped-entities": 0,
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "no-extra-boolean-cast": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "moment": "2.29.1"
  },
  "devDependencies": {
    "babel-plugin-prismjs": "^2.1.0"
  }
}
