    "lord-icon-element": "^3.3.3",
    "lottie-web": "^5.8.1",
    "node-sass": "^7.0.1",
    "nouislider-react": "^3.4.1",
    "prismjs": "^1.26.0",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-color": "^2.19.3",
    "react-countdown": "^2.3.2",
    "react-countup": "^6.1.1",
    "react-dom": "^17.0.2",
    "react-drag-listview": "^0.1.9",
    "react-dropzone": "^12.0.1",
    "react-dual-listbox": "^2.2.0",
    "react-flatpickr": "^3.10.7",
    "react-image-lightbox": "^5.1.4",
